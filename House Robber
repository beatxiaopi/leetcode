class Solution {
    public int rob(int[] nums) {
        int n=nums.length, robNext=nums[n-1];
        int robNextPlusOne=0;
        for (int i=n-2; i>=0; --i) {
            int current=Math.max(robNext, robNextPlusOne+nums[i]);
            robNextPlusOne=robNext;
            robNext=current;
        }
        return robNext;
    }
}
